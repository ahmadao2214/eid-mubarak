{
  "expo": {
    "name": "EidMemeMaker",
    "slug": "eid-meme-maker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "eid-meme-maker",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.eidmememaker.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1a2e"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.eidmememaker.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "EidMemeMaker needs access to your photos to create meme cards.",
          "cameraPermission": "EidMemeMaker needs access to your camera to take photos for meme cards."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "EidMemeMaker needs access to save videos to your camera roll.",
          "savePhotosPermission": "EidMemeMaker needs permission to save videos to your camera roll."
        }
      ]
    ]
  }
}
